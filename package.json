  "scripts": {
    "build": "tsc",
    "check": "npm run tslint",
    "clean": "rimraf ./dst ./docs ./coverage \"npm-debug.log*\" ./.nyc_output",
    "doc": "echo \"No docs.\" && exit 0",
    "swagger": "cpx ./swagger.yaml docs && npm run swagger2blueprint && npm run blueprint2doc",
    "swagger2blueprint": "swagger2blueprint ./docs/swagger.yaml ./docs/index.apib",
    "blueprint2doc": "aglio --theme-variables streak --theme-condense-nav true --theme-full-width true --theme-template triple --theme-style default -i ./docs/index.apib -o ./docs/index.html",
    "http-server": "http-server",
    "mocha": "mocha",
    "coverage": "nyc mocha \"src/**/*.spec.ts\"",
    "start": "node dst/server.js",
    "start:development": "node dst/ssl.js",
    "test": "npm run check && npm run coverage",
    "tslint": "tslint --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" \"src/**/*.ts\"",
    "preversion": "npm run clean && npm run build && npm test && npm run doc",
    "version": "git add -A",
    "postversion": "git push origin --tags"
  },
  "version": "5.19.0-alpha.1"
